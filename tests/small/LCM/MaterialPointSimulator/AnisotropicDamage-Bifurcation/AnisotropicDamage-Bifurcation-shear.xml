
<ParameterList>
  <!-- <Parameter name="Reference Material" type="string" value="Metal"/> -->
  <ParameterList name="ElementBlocks">

    <ParameterList name="Block0">
      <Parameter
        name="material"
        type="string"
        value="Hydride" />
      <Parameter
        name="Weighted Volume Average J"
        type="bool"
        value="true" />
      <Parameter
        name="Average J Stabilization Parameter"
        type="double"
        value="0.05" />
    </ParameterList>
  </ParameterList>

  <ParameterList name="Materials">

    <ParameterList name="Hydride">
      <ParameterList name="Material Model">
        <Parameter
          name="Model Name"
          type="string"
          value="Anisotropic Damage" />
      </ParameterList>
      <ParameterList name="Elastic Modulus">
        <Parameter
          name="Elastic Modulus Type"
          type="string"
          value="Constant" />
        <Parameter
          name="Value"
          type="double"
          value="200" />
      </ParameterList>
      <ParameterList name="Poissons Ratio">
        <Parameter
          name="Poissons Ratio Type"
          type="string"
          value="Constant" />
        <Parameter
          name="Value"
          type="double"
          value="0.25" />
      </ParameterList>
      <Parameter
        name="Matrix volume fraction"
        type="double"
        value="0.4" />
      <Parameter
        name="Matrix maximum damage"
        type="double"
        value="1.0" />
      <Parameter
        name="Matrix damage saturation"
        type="double"
        value="4.0" />
      <Parameter
        name="Fiber 1 k"
        type="double"
        value="100" />
      <Parameter
        name="Fiber 1 q"
        type="double"
        value="1.0" />
      <Parameter
        name="Fiber 1 volume fraction"
        type="double"
        value="0.3" />
      <Parameter
        name="Fiber 1 maximum damage"
        type="double"
        value="1.0" />
      <Parameter
        name="Fiber 1 damage saturation"
        type="double"
        value="4.0" />
      <Parameter
        name="Fiber 2 k"
        type="double"
        value="100" />
      <Parameter
        name="Fiber 2 q"
        type="double"
        value="1.0" />
      <Parameter
        name="Fiber 2 volume fraction"
        type="double"
        value="0.3" />
      <Parameter
        name="Fiber 2 maximum damage"
        type="double"
        value="1.0" />
      <Parameter
        name="Fiber 2 damage saturation"
        type="double"
        value="4.0" />
      <Parameter
        name="Fiber 1 Orientation Vector"
        type="Array(double)"
        value="{0.8, 0.6, 0.0}" />
      <Parameter
        name="Fiber 2 Orientation Vector"
        type="Array(double)"
        value="{0.8, -0.6, 0.0}" />

      <!-- Output -->
      <Parameter
        name="Output Cauchy Stress"
        type="bool"
        value="true" />
      <Parameter
        name="Output Matrix Energy"
        type="bool"
        value="true" />
      <Parameter
        name="Output Matrix Damage"
        type="bool"
        value="true" />
      <Parameter
        name="Output Fiber 1 Energy"
        type="bool"
        value="true" />
      <Parameter
        name="Output Fiber 1 Damage"
        type="bool"
        value="true" />
      <Parameter
        name="Output Fiber 2 Energy"
        type="bool"
        value="true" />
      <Parameter
        name="Output Fiber 2 Damage"
        type="bool"
        value="true" />

      <ParameterList name="Material Point Simulator">
        <!-- Bifurcation -->
        <Parameter
          name="Check Stability"
          type="bool"
          value="true" />
        <Parameter
          name="Parametrization Type"
          type="string"
          value="Cartesian" />
        <Parameter
          name="Parametrization Interval"
          type="double"
          value="0.05" />
        <Parameter
          name="Adaptive Step Output File Name"
          type="string"
          value="Bifurcation-Adaptive.txt" />

        <!-- Loading Parameters -->
        <Parameter
          name="Loading Case Name"
          type="string"
          value="simple-shear" />
        <Parameter
          name="Number of Steps"
          type="int"
          value="80" />
        <Parameter
          name="Step Size"
          type="double"
          value="0.01" />
        <Parameter
          name="Output File Name"
          type="string"
          value="AnisotropicDamage-Bifurcation-shear.exo" />
      </ParameterList>

    </ParameterList>

  </ParameterList>

</ParameterList>










