<ParameterList>

  <!-- Assign materials to element blocks ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

  <ParameterList name="ElementBlocks">

    <ParameterList name="block_2">

      <Parameter
        name="material"
        type="string"
        value="FCC" />

      <Parameter
        name="Weighted Volume Average J"
        type="bool"
        value="true" />

      <Parameter
        name="Volume Average Pressure"
        type="bool"
        value="true" />

    </ParameterList>

  </ParameterList>

  <!-- Define materials ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

  <ParameterList name="Materials">

    <ParameterList name="FCC">

      <ParameterList name="Material Model">

        <Parameter
          name="Model Name"
          type="string"
          value="CrystalPlasticity" />

      </ParameterList>

      <!-- ~~~~~~~~~ Crystal Elasticity moduli and orientation ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

      <ParameterList name="Crystal Elasticity">

        <Parameter
          name="C11"
          type="double"
          value="204.6e3" />

        <Parameter
          name="C12"
          type="double"
          value="137.7e3" />

        <Parameter
          name="C44"
          type="double"
          value="126.2e3" />

        <Parameter
          name="Basis Vector 1"
          type="Array(double)"
          value="{1.0, 0.0, 0.0}" />

        <Parameter
          name="Basis Vector 2"
          type="Array(double)"
          value="{0.0, 1.0, 0.0}" />

        <Parameter
          name="Basis Vector 3"
          type="Array(double)"
          value="{0.0, 0.0, 1.0}" />

      </ParameterList>

      <!-- ~~~~~~~~~ Set integration scheme and solver options ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

      <Parameter
        name="Integration Scheme"
        type="string"
        value="Implicit" />

      <Parameter
        name="Nonlinear Solver Step Type"
        type="string"
        value="Newton" />

      <Parameter
        name="Implicit Integration Relative Tolerance"
        type="double"
        value="1.0e-35" />

      <Parameter
        name="Implicit Integration Absolute Tolerance"
        type="double"
        value="1.0e-12" />

      <Parameter
        name="Implicit Integration Max Iterations"
        type="int"
        value="100" />

      <Parameter
        name="Output CP_Residual"
        type="bool"
        value="true" />

      <!-- ~~~~~~~~~ Set flow rule type and parameters ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

      <ParameterList name="Slip System Family 0">

        <ParameterList name="Flow Rule">

          <Parameter
            name="Type"
            type="string"
            value="Power Law" />

          <Parameter
            name="Reference Slip Rate"
            type="double"
            value="1.0" />

          <Parameter
            name="Rate Exponent"
            type="double"
            value="2.0e01" />

        </ParameterList>

        <!-- ~~~~~~~~~ Set hardening law type and parameters ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

        <ParameterList name="Hardening Law">

          <Parameter
            name="Type"
            type="string"
            value="Linear Minus Recovery" />

          <Parameter
            name="Hardening Modulus"
            type="double"
            value="3.55e02" />

          <Parameter
            name="Recovery Modulus"
            type="double"
            value="2.9e00" />

          <Parameter
            name="Initial Hardening State"
            type="double"
            value="1.22e02" />

        </ParameterList>

      </ParameterList>

      <!-- ~~~~~~~~~ Define crystal plasticity slip systems ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      <!-- ~~~~~~~~~ Convention taken from Plasticity by Nemat-Nasser, p. 454 - 455 
        ~~~~~~~~~~~~~~~~~~ -->

      <Parameter
        name="Number of Slip Systems"
        type="int"
        value="12" />

      <!-- ~~~~~~~~~ Slip system 1 ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

      <ParameterList name="Slip System 1">

        <Parameter
          name="Slip Direction"
          type="Array(double)"
          value="{-1.0,1.0,0.0}" />

        <Parameter
          name="Slip Normal"
          type="Array(double)"
          value="{1.0,1.0,1.0}" />

      </ParameterList>

