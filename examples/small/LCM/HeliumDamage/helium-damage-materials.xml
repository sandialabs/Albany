  <ParameterList name="ElementBlocks">

    <ParameterList name="Block0">
      <Parameter
        name="material"
        type="string"
        value="21-6-9" />
      <Parameter
        name="Weighted Volume Average J"
        type="bool"
        value="true" />
      <Parameter
        name="Volume Average Pressure"
        type="bool"
        value="true" />

      <Parameter
        name="Output Trapped_Concentration"
        type="bool"
        value="true" />
      <Parameter
        name="Output Total_Concentration"
        type="bool"
        value="true" />
      <Parameter
        name="Output He_Concentration"
        type="bool"
        value="true" />
      <Parameter
        name="Output Total_Bubble_Density"
        type="bool"
        value="true" />
      <Parameter
        name="Output Bubble_Volume_Fraction"
        type="bool"
        value="true" />

    </ParameterList>
  </ParameterList>


  <ParameterList name="Materials">

    <ParameterList name="21-6-9">
      <Parameter
        name="Output Convergence"
        type="bool"
        value="false" />

      <ParameterList name="Material Model">
        <Parameter
          name="Model Name"
          type="string"
          value="Elasto Viscoplastic" />
      </ParameterList>

      <!-- Stabilization Parameter - 2.0 is equivalent to lumped mass -->
      <Parameter
        name="Stabilization Parameter"
        type="double"
        value="2.0" />

      <!-- Initial Concentration for this block -->
      <Parameter
        name="Initial Concentration"
        type="double"
        value="5.6e-4" />

      <!-- Hydorgen Isotope Transport Parameters -->
      <ParameterList name="Transport Coefficients">
        <Parameter
          name="Partial Molar Volume"
          type="double"
          value="2.0" />
        <Parameter
          name="Ideal Gas Constant"
          type="double"
          value="0.008314" />
        <Parameter
          name="Pre-exponential Factor"
          type="double"
          value="5.4e5" />
        <Parameter
          name="Diffusion Activation Enthalpy"
          type="double"
          value="53.9" />
        <Parameter
          name="Trap Binding Energy"
          type="double"
          value="9.65" />
        <Parameter
          name="Number of Lattice Sites"
          type="double"
          value="0.1405283867341203" />
        <Parameter
          name="Reference Total Concentration"
          type="double"
          value="5.6e-4" />
        <Parameter
          name="Lattice Strain Flag"
          type="bool"
          value="false" />
        <Parameter
          name="A Constant"
          type="double"
          value="8.6" />
        <Parameter
          name="B Constant"
          type="double"
          value="1.5" />
        <Parameter
          name="C Constant"
          type="double"
          value="6.96" />
        <Parameter
          name="Avogadro's Number"
          type="double"
          value="6.0221413e11" />
      </ParameterList>

      <!-- Tritium Decay Parameters -->
      <ParameterList name="Tritium Coefficients">
        <Parameter
          name="Tritium Decay Constant"
          type="double"
          value="1.79e-9" />
        <Parameter
          name="Helium Radius"
          type="double"
          value="2.5e-4" />
        <Parameter
          name="Atoms Per Cluster"
          type="double"
          value="10" />
        <Parameter
          name="Evaluate HeliumODEs"
          type="bool"
          value="true" />
      </ParameterList>
      <ParameterList name="Molar Volume">
        <Parameter
          name="Type"
          type="string"
          value="Constant" />
        <Parameter
          name="Value"
          type="double"
          value="7.116" />
      </ParameterList>

      <!-- Temperature Parameters -->
      <ParameterList name="Thermal Conductivity">
        <Parameter
          name="Thermal Conductivity Type"
          type="string"
          value="Constant" />
        <Parameter
          name="Value"
          type="double"
          value="0.0" />
      </ParameterList>
      <!-- FIXME: Double check these for 21-6-9 in proper units -->
      <Parameter
        name="Reference Temperature"
        type="double"
        value="300.0" />
      <Parameter
        name="Initial Temperature"
        type="double"
        value="300.0" />
      <Parameter
        name="Thermal Transient Coefficient"
        type="double"
        value="1.0" />
      <Parameter
        name="Thermal Expansion Coefficient"
        type="double"
        value="0.0" />
      <Parameter
        name="Density"
        type="double"
        value="7.806e-18" />
      <Parameter
        name="Heat Capacity"
        type="double"
        value="1.0" />

      <!-- Mechanical Parameters -->
      <ParameterList name="Elastic Modulus">
        <Parameter
          name="Elastic Modulus Type"
          type="string"
          value="Constant" />
        <Parameter
          name="Value"
          type="double"
          value="196.0" />
        <Parameter
          name="Reference Temperature"
          type="double"
          value="300.0" />
        <Parameter
          name="Linear Temperature Coefficient"
          type="double"
          value="-0.0" />
      </ParameterList>
      <ParameterList name="Poissons Ratio">
        <Parameter
          name="Poissons Ratio Type"
          type="string"
          value="Constant" />
        <Parameter
          name="Value"
          type="double"
          value="0.3" />
        <Parameter
          name="Reference Temperature"
          type="double"
          value="300.0" />
        <Parameter
          name="Linear Temperature Coefficient"
          type="double"
          value="-0.0" />
      </ParameterList>
      <ParameterList name="Yield Strength">
        <Parameter
          name="Yield Strength Type"
          type="string"
          value="Constant" />
        <Parameter
          name="Value"
          type="double"
          value="0.714" />
        <Parameter
          name="Reference Temperature"
          type="double"
          value="300.0" />
        <Parameter
          name="Linear Temperature Coefficient"
          type="double"
          value="-0.0" />
      </ParameterList>
      <ParameterList name="Hardening Modulus">
        <Parameter
          name="Hardening Modulus Type"
          type="string"
          value="Constant" />
        <Parameter
          name="Value"
          type="double"
          value="0.0157" />
        <Parameter
          name="Temperature Dependence Type"
          type="string"
          value="Arrhenius" />
        <Parameter
          name="Pre Exponential"
          type="double"
          value="0.0157" />
        <Parameter
          name="Exponential Parameter"
          type="double"
          value="0.0" />
      </ParameterList>
      <ParameterList name="Recovery Modulus">
        <Parameter
          name="Recovery Modulus Type"
          type="string"
          value="Constant" />
        <Parameter
          name="Value"
          type="double"
          value="1.96" />
        <Parameter
          name="Temperature Dependence Type"
          type="string"
          value="Arrhenius" />
        <Parameter
          name="Pre Exponential"
          type="double"
          value="1.96" />
        <Parameter
          name="Exponential Parameter"
          type="double"
          value="0.0" />
      </ParameterList>
      <ParameterList name="Flow Rule Coefficient">
        <Parameter
          name="Flow Rule Coefficient Type"
          type="string"
          value="Constant" />
        <Parameter
          name="Value"
          type="double"
          value="100.0" />
      </ParameterList>
      <ParameterList name="Flow Rule Exponent">
        <Parameter
          name="Flow Rule Exponent Type"
          type="string"
          value="Constant" />
        <Parameter
          name="Value"
          type="double"
          value="1.0" />
      </ParameterList>

      <!-- Gurson failure surface parameters -->
      <Parameter
        name="Initial Void Volume"
        type="double"
        value="0.001" />
      <Parameter
        name="Shear Damage Parameter"
        type="double"
        value="0.0" />
      <Parameter
        name="Void Nucleation Parameter fN"
        type="double"
        value="0.0" />
      <Parameter
        name="Void Nucleation Parameter sN"
        type="double"
        value="0.1" />
      <Parameter
        name="Void Nucleation Parameter eN"
        type="double"
        value="0.3" />
      <Parameter
        name="Critical Void Volume"
        type="double"
        value="0.15" />
      <Parameter
        name="Failure Void Volume"
        type="double"
        value="0.25" />
      <Parameter
        name="Yield Parameter q1"
        type="double"
        value="1.5" />
      <Parameter
        name="Yield Parameter q2"
        type="double"
        value="1.0" />
      <Parameter
