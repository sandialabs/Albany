        type="double"
        value="0.005" />
      <Parameter
        name="Mean Strain Hydrogen Linear Coefficient"
        type="double"
        value="-10.0" />
      <Parameter
        name="Hydrogen Nucleation Standard Deviation"
        type="double"
        value="0.1" />
      <Parameter
        name="Void Volume Fraction Nucleation Parameter"
        type="double"
        value="0.01" />
      <Parameter
        name="Void Volume Fraction He Linear Coefficient"
        type="double"
        value="1000.0" />

      <!-- Output -->
      <Parameter
        name="Output Cauchy Stress"
        type="bool"
        value="true" />
      <Parameter
        name="Output eqps"
        type="bool"
        value="true" />
      <Parameter
        name="Output eps_ss"
        type="bool"
        value="true" />
      <Parameter
        name="Output kappa"
        type="bool"
        value="true" />
      <Parameter
        name="Output void volume fraction"
        type="bool"
        value="true" />

