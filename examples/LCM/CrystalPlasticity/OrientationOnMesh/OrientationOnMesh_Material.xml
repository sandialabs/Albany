                          type="bool" 
                          value="true" />

               <Parameter name="Volume Average Pressure" 
                          type="bool" 
                          value="true" />

          </ParameterList>

     </ParameterList>

     <ParameterList name="Materials">

          <ParameterList name="FCC">

               <ParameterList name="Material Model"> 

                    <Parameter name="Model Name" 
                               type="string" 
                               value="CrystalPlasticity"/>

               </ParameterList>

<!-- ~~~~~~~~~ Crystal Elasticity moduli and orientation -->

               <Parameter name="Read Lattice Orientation From Mesh"
                          type="bool" 
                          value="true"/>

               <ParameterList name="Crystal Elasticity">

                    <Parameter name="C11" 
                               type="double" 
                               value="204.6e3"/>

                    <Parameter name="C12" 
                               type="double" 
                               value="137.7e3"/>

                    <Parameter name="C44" 
                               type="double" 
                               value="126.2e3"/>

               </ParameterList>

               <Parameter name="Integration Scheme" 
                          type="string" 
                          value="Implicit"/>

               <Parameter name="Implicit Integration Relative Tolerance" 
                          type="double" 
                          value="1.0e-35"/>

               <Parameter name="Implicit Integration Absolute Tolerance" 
                          type="double" 
                          value="1.0e-12"/>

               <Parameter name="Implicit Integration Max Iterations" 
                          type="int" 
                          value="100"/>

               <Parameter name="Output CP_Residual" 
                          type="bool" 
                          value="true"/>

               <ParameterList name="Flow Rule">

                    <Parameter name="Type" 
                               type="string" 
                               value="Power Law"/>

                    <Parameter name="Gamma Dot" 
                               type="double" 
                               value="1.0e00"/>

                    <Parameter name="Gamma Exponent" 
                               type="double" 
                               value="2.0e01"/>

               </ParameterList>

               <ParameterList name="Hardening Law">

                    <Parameter name="Type" 
                               type="string" 
                               value="Exponential"/>

                    <Parameter name="Hardening" 
                               type="double" 
                               value="3.55e02"/>

                    <Parameter name="Hardening Exponent" 
                               type="double" 
                               value="2.9e00"/>

                    <Parameter name="Tau Critical"
                               type="double"
                               value="1.22e02"/>

               </ParameterList>

