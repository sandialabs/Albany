<ParameterList>
  <ParameterList name="ElementBlocks">

    <ParameterList name="block_1">
      <Parameter name="material" type="string" value="Aligned Lattice" />
    </ParameterList>

    <ParameterList name="block_2">
      <Parameter name="material" type="string" value="FCC" />
    </ParameterList>

  </ParameterList>

  <ParameterList name="Materials">

    <ParameterList name="Aligned Lattice">

      <ParameterList name="Material Model"> 
        <Parameter name="Model Name" type="string" value="CrystalPlasticity"/>
      </ParameterList>

      <Parameter name="Integration Scheme" type="string" value="Implicit"/>

      <Parameter name="Implicit Integration Relative Tolerance" type="double" value="1.0e-10"/>
`
      <Parameter name="Implicit Integration Max Iterations Implicit" type="int" value="100"/>

      <Parameter name="Output CP_Residual" type="bool" value="true"/>

      <Parameter name="Output CP_Residual_Iter" type="bool" value="true"/>

      <!-- Crystal Elasticity moduli and orientation -->
      <ParameterList name="Crystal Elasticity">
                    <Parameter name="C11"
                               type="double"
                               value="204.6e3"/>

                    <Parameter name="C12"
                               type="double"
                               value="137.7e3"/>

                    <Parameter name="C44"
                               type="double"
                               value="126.2e3"/>
                    
                    <Parameter name="Basis Vector 1" 
                               type="Array(double)" 
                               value="{1.0,0.0,0.0}"/>
 
                    <Parameter name="Basis Vector 2" 
                               type="Array(double)" 
                               value="{0.0,1.0,0.0}"/>
 
                    <Parameter name="Basis Vector 3" 
                               type="Array(double)" 
                               value="{0.0,0.0,1.0}"/>

      </ParameterList>

      <ParameterList name="Flow Rule">

                    <Parameter name="Type" 
                               type="string" 
                               value="Power Law with Drag"/>

                    <Parameter name="Gamma Dot" 
                               type="double" 
                               value="1.0e0"/>

                    <Parameter name="Gamma Exponent" 
                               type="double" 
                               value="1.0e2"/>

		    <Parameter name="Drag Coefficient" 
                               type="double" 
                               value="1.0e1"/>

      </ParameterList>

      <ParameterList name="Hardening Law">

                    <Parameter name="Type" 
                               type="string" 
                               value="Exponential"/>

                    <Parameter name="Hardening" 
                               type="double" 
                               value="0.0"/>

                    <Parameter name="Hardening Exponent" 
                               type="double" 
                               value="0.0"/>

                    <Parameter name="Tau Critical"
                               type="double"
                               value="1.22e02"/>

      </ParameterList>

      <!-- Crystal Plasticity Slip System -->
      <Parameter name="Number of Slip Systems" type="int" value="1"/>

      <ParameterList name="Slip System 1">
      
                    <Parameter name="Slip Direction"
                               type="Array(double)"
                               value="{1.0, 0.0, 0.0}"/>

                    <Parameter name="Slip Normal"
                               type="Array(double)"
                               value="{0.0, 1.0, 0.0}"/>


          <Parameter name="Flow Rule" type="string" value="Flow Rule"/>
          <Parameter name="Hardening Law" type="string" value="Hardening Law"/>

      </ParameterList>

      <!-- Specify output to Exodus -->
      <Parameter name="Output Cauchy Stress" type="bool" value="true"/>
      <Parameter name="Output Deformation Gradient" type="bool" value="true"/>
      <Parameter name="Output Fp" type="bool" value="true"/>
      <Parameter name="Output gamma_1" type="bool" value="true"/>
      <Parameter name="Output tau_hard_1" type="bool" value="true"/>
      <Parameter name="Output tau_1" type="bool" value="true"/>
      <Parameter name="Output CP_Residual" type="bool" value="true"/>
      <Parameter name="Output CP_Residual_Iter" type="bool" value="true"/>
    </ParameterList>

    <ParameterList name="FCC">

      <ParameterList name="Material Model"> 
        <Parameter name="Model Name" type="string" value="CrystalPlasticity"/>
      </ParameterList>

      <Parameter name="Integration Scheme" type="string" value="Implicit"/>
      <Parameter name="Implicit Integration Relative Tolerance" type="double" value="1.0e-35"/>
      <Parameter name="Implicit Integration Absolute Tolerance" type="double" value="1.0e-14"/>
      <Parameter name="Implicit Integration Max Iterations Implicit" type="int" value="100"/>

      <!-- Crystal Elasticity moduli and orientation -->
      <ParameterList name="Crystal Elasticity">
        <Parameter name="C11" type="double" value="204.6e3"/>
        <Parameter name="C12" type="double" value="137.7e3"/>
        <Parameter name="C44" type="double" value="126.2e3"/>

        <Parameter name="Basis Vector 1" 
                   type="Array(double)" 
                   value="{-0.09175170953613698, 0.9082482904638630, 0.4082482904638630}"/>

        <Parameter name="Basis Vector 2" 
                   type="Array(double)" 
                   value="{0.9082482904638630, -0.09175170953613698, 0.4082482904638630}"/>

        <Parameter name="Basis Vector 3" 
                   type="Array(double)" 
                   value="{0.4082482904638630, 0.4082482904638630, -0.8164965809277260}"/>
      </ParameterList>

      
      <ParameterList name="Flow Rule">

                    <Parameter name="Type" 
                               type="string" 
                               value="Power Law with Drag"/>

                    <Parameter name="Gamma Dot" 
                               type="double" 
                               value="1.0e00"/>

                    <Parameter name="Gamma Exponent" 
                               type="double" 
                               value="1.0e02"/>
		    
		    <Parameter name="Drag Coefficient" 
                               type="double" 
                               value="1.0e1"/>

      </ParameterList>

      <ParameterList name="Hardening Law">

                    <Parameter name="Type" 
                               type="string" 
                               value="Exponential"/>

                    <Parameter name="Hardening" 
                               type="double" 
                               value="0.0"/>

                    <Parameter name="Hardening Exponent" 
                               type="double" 
                               value="0.0"/>

                    <Parameter name="Tau Critical"
                               type="double"
                               value="1.22e02"/>

      </ParameterList>

      <!-- Crystal Plasticity Slip System -->
      <Parameter name="Number of Slip Systems" type="int" value="1"/>

      <!-- Crystal Plasticity Slip System -->
      <Parameter name="Number of Slip Systems" type="int" value="1"/>
      <ParameterList name="Slip System 1">
        <Parameter name="Slip Direction" type="Array(double)" value="{-1.0, 1.0, 0.0}"/>
        <Parameter name="Slip Normal" type="Array(double)"    value="{1.0, 1.0, 1.0}"/>
        <Parameter name="Flow Rule" type="string" value="Flow Rule"/>
        <Parameter name="Hardening Law" type="string" value="Hardening Law"/>
      </ParameterList>

      <!-- Specify output to Exodus -->
      <Parameter name="Output Cauchy Stress" type="bool" value="true"/>
      <Parameter name="Output Deformation Gradient" type="bool" value="true"/>
      <Parameter name="Output Fp" type="bool" value="true"/>
      <Parameter name="Output gamma_1" type="bool" value="true"/>
      <Parameter name="Output tau_hard_1" type="bool" value="true"/>
      <Parameter name="Output tau_1" type="bool" value="true"/>
      <Parameter name="Output CP_Residual" type="bool" value="true"/>
      <Parameter name="Output CP_Residual_Iter" type="bool" value="true"/>
    </ParameterList>

  </ParameterList>

</ParameterList>
