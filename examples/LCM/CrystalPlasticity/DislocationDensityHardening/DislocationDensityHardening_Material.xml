<ParameterList>

  <ParameterList name="ElementBlocks">

    <ParameterList name="element">

      <Parameter
        name="material"
        type="string"
        value="FCC" />

      <Parameter
        name="Weighted Volume Average J"
        type="bool"
        value="true" />

      <Parameter
        name="Volume Average Pressure"
        type="bool"
        value="true" />

      <Parameter
        name="Output Integration Weights"
        type="bool"
        value="true" />

    </ParameterList>

  </ParameterList>

  <ParameterList name="Materials">

    <ParameterList name="FCC">

      <ParameterList name="Material Model">

        <Parameter
          name="Model Name"
          type="string"
          value="CrystalPlasticity" />

      </ParameterList>


      <!-- ~~~~~~~~~ Crystal Elasticity moduli and orientation ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

      <ParameterList name="Crystal Elasticity">

        <Parameter
          name="C11"
          type="double"
          value="267.0e3" />

        <Parameter
          name="C12"
          type="double"
          value="161.0e3" />

        <Parameter
          name="C44"
          type="double"
          value="82.5e3" />

        <Parameter
          name="Basis Vector 1"
          type="Array(double)"
          value="{1.0, 1.0, 0.0}" />

        <Parameter
          name="Basis Vector 2"
          type="Array(double)"
          value="{-1.0, 1.0, 0.0}" />

        <Parameter
          name="Basis Vector 3"
          type="Array(double)"
          value="{0.0, 0.0, 1.0}" />

      </ParameterList>


      <!-- ~~~~~~~~~ Set model options ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

      <Parameter
        name="Integration Scheme"
        type="string"
        value="Implicit" />

      <Parameter
        name="Residual Type"
        type="string"
        value="Slip Hardness" />

      <Parameter
        name="Implicit Integration Relative Tolerance"
        type="double"
        value="1.0e-35" />

      <Parameter
        name="Implicit Integration Absolute Tolerance"
        type="double"
        value="1.0e-10" />

      <Parameter
        name="Implicit Integration Max Iterations"
        type="int"
        value="100" />

      <Parameter
        name="Output CP_Residual"
        type="bool"
        value="true" />

      <Parameter
        name="Verbosity"
        type="int"
        value="0" />

      <ParameterList name="Slip System Family 0">

        <ParameterList name="Flow Rule">

          <Parameter
            name="Type"
            type="string"
            value="Power Law" />

          <Parameter
            name="Reference Slip Rate"
            type="double"
            value="1.0e-03" />

          <Parameter
            name="Rate Exponent"
            type="double"
            value="8.0e01" />

        </ParameterList>

        <ParameterList name="Hardening Law">

          <Parameter
            name="Type"
            type="string"
            value="Dislocation Density" />

          <Parameter
            name="Geometric Factor"
            type="double"
            value="4.0e-01" />

          <!-- Units must be consistent with Burgers Vector Magnitude
                 1/m^2: 8e13, 1/mm^2: 8e7, 1/um^2: 8e1 -->

          <Parameter
            name="Initial Hardening State"
            type="double"
            value="8.0e+01" />

          <!-- Units must be consistent with Initial Hardening State
                 1/m^2: 5e15, 1/mm^2: 5e9, 1/um^2: 5e3 
               The saturated hardening state is given by the ratio
                 (Generation Factor / Annihilation Factor)^2
               The rate of saturation is governed by the magnitude a
                 Generation Factor = a * (Generation Factor)_0
                 Annihilation Factor = a * (Annihilation Factor)_0 -->

          <Parameter
            name="Generation Factor"
            type="double"
            value="5.0e+03" />

          <Parameter
            name="Annihilation Factor"
            type="double"
            value="5.0e+02" />

          <!-- Units must be consistent with elastic constants
                 N/mm^2 (MPa): 7.07e+04 -->

          <Parameter
            name="Shear Modulus"
            type="double"
            value="7.07e+04" />

          <!-- Units must be consistent with Initial Hardening State
                 m:  2.87e-10, mm: 2.87e-7, um: 2.87e-4 -->

          <Parameter
            name="Burgers Vector Magnitude"
            type="double"
            value="2.87e-04" />

        </ParameterList>

      </ParameterList>


      <!-- ~~~~~~~~~ Definitions of hardening laws ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

      <ParameterList name="Exponential Hardening">

        <Parameter
          name="Type"
          type="string"
          value="Exponential" />

        <Parameter
          name="Hardening"
          type="double"
          value="3.55e02" />

        <Parameter
          name="Hardening Exponent"
          type="double"
          value="2.9e00" />

        <Parameter
          name="Tau Critical"
          type="double"
          value="1.22e02" />

      </ParameterList>

      <!-- ~~~~~~~~~ Crystal Plasticity Slip Systems ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
      <!-- ~~~~~~~~~ Convention taken from Plasticity by Nemat-Nasser, p. 454 - 455 
        ~~~~~~~~~~~~~~~~~~ -->

      <Parameter
        name="Number of Slip Systems"
        type="int"
        value="12" />

      <ParameterList name="Slip System 1">

        <Parameter
          name="Slip Direction"
          type="Array(double)"
          value="{-1.0,1.0,0.0}" />

        <Parameter
          name="Slip Normal"
          type="Array(double)"
          value="{1.0,1.0,1.0}" />

      </ParameterList>


      <ParameterList name="Slip System 2">

