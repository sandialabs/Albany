                    <Parameter name="Hardening Law" 
                               type="string" 
                               value="Saturation Hardening"/> 

               </ParameterList>


               <ParameterList name="Slip System 5">

                    <Parameter name="Slip Direction" 
                               type="Array(double)" 
                               value="{1.0,0.0,1.0}"/>

                    <Parameter name="Slip Normal" 
                               type="Array(double)"    
                               value="{-1.0,1.0,1.0}"/>

                    <Parameter name="Flow Rule" 
                               type="string" 
                               value="Flow Rule"/>

                    <Parameter name="Hardening Law" 
                               type="string" 
                               value="Saturation Hardening"/> 

               </ParameterList>


               <ParameterList name="Slip System 6">

                    <Parameter name="Slip Direction" 
                               type="Array(double)" 
                               value="{0.0,1.0,-1.0}"/>

                    <Parameter name="Slip Normal" 
                               type="Array(double)"    
                               value="{-1.0,1.0,1.0}"/>

                    <Parameter name="Flow Rule" 
                               type="string" 
                               value="Flow Rule"/>

                    <Parameter name="Hardening Law" 
                               type="string" 
                               value="Saturation Hardening"/> 

               </ParameterList>


               <ParameterList name="Slip System 7">

                    <Parameter name="Slip Direction" 
                               type="Array(double)" 
                               value="{1.0,-1.0,0.0}"/>

                    <Parameter name="Slip Normal" 
                               type="Array(double)"    
                               value="{-1.0,-1.0,1.0}"/>

                    <Parameter name="Flow Rule" 
                               type="string" 
                               value="Flow Rule"/>

                    <Parameter name="Hardening Law" 
                               type="string" 
                               value="Saturation Hardening"/> 

               </ParameterList>


               <ParameterList name="Slip System 8">

                    <Parameter name="Slip Direction" 
                               type="Array(double)" 
                               value="{0.0,1.0,1.0}"/>

                    <Parameter name="Slip Normal" 
                               type="Array(double)"    
                               value="{-1.0,-1.0,1.0}"/>

                    <Parameter name="Flow Rule" 
                               type="string" 
                               value="Flow Rule"/>

                    <Parameter name="Hardening Law" 
                               type="string" 
                               value="Saturation Hardening"/>

               </ParameterList>


               <ParameterList name="Slip System 9">

                    <Parameter name="Slip Direction" 
                               type="Array(double)" 
                               value="{-1.0,0.0,-1.0}"/>

                    <Parameter name="Slip Normal" 
                               type="Array(double)"    
                               value="{-1.0,-1.0,1.0}"/>

                    <Parameter name="Flow Rule" 
                               type="string" 
                               value="Flow Rule"/>

                    <Parameter name="Hardening Law" 
                               type="string" 
                               value="Saturation Hardening"/> 

               </ParameterList>


               <ParameterList name="Slip System 10">

                    <Parameter name="Slip Direction" 
                               type="Array(double)" 
                               value="{1.0,1.0,0.0}"/>

                    <Parameter name="Slip Normal" 
                               type="Array(double)"    
                               value="{1.0,-1.0,1.0}"/>

                    <Parameter name="Flow Rule" 
                               type="string" 
                               value="Flow Rule"/>

                    <Parameter name="Hardening Law" 
                               type="string" 
                               value="Saturation Hardening"/>

               </ParameterList>


               <ParameterList name="Slip System 11">

                    <Parameter name="Slip Direction" 
                               type="Array(double)" 
                               value="{-1.0,0.0,1.0}"/>

                    <Parameter name="Slip Normal" 
                               type="Array(double)"    
                               value="{1.0,-1.0,1.0}"/>

                    <Parameter name="Flow Rule" 
                               type="string" 
                               value="Flow Rule"/>

                    <Parameter name="Hardening Law" 
                               type="string" 
                               value="Saturation Hardening"/> 

               </ParameterList>


               <ParameterList name="Slip System 12">

                    <Parameter name="Slip Direction" 
                               type="Array(double)" 
                               value="{0.0,-1.0,-1.0}"/>

                    <Parameter name="Slip Normal" 
                               type="Array(double)"    
                               value="{1.0,-1.0,1.0}"/>

                    <Parameter name="Flow Rule" 
                               type="string" 
                               value="Flow Rule"/>

                    <Parameter name="Hardening Law" 
                               type="string" 
                               value="Saturation Hardening"/> 

               </ParameterList>

<!-- ~~~~~~~~~ Specify output to Exodus ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

               <Parameter name="Output Cauchy Stress" 
                          type="bool" 
                          value="true"/>

               <Parameter name="Output Deformation Gradient" 
                          type="bool" 
                          value="true"/>

               <Parameter name="Output Fp" 
                          type="bool" 
