
rm -rf CMake* 

TRILINSTALLDIR="/home/projects/albany/nightlyCDashTrilinosBlake/build-gcc/TrilinosSerialInstallGccNoWarn"

cmake \
    -D ALBANY_TRILINOS_DIR:FILEPATH=${TRILINSTALLDIR} \
    -D ENABLE_LANDICE:BOOL=ON \
    -D ENABLE_DEMO_PDES:BOOL=ON \
    -D ENABLE_KOKKOS_UNDER_DEVELOPMENT:BOOL=ON \
    -D ALBANY_CTEST_TIMEOUT=2400 \
    -D ENABLE_CHECK_FPE:BOOL=OFF \
    -D ALBANY_MPI_EXEC_TRAILING_OPTIONS='--map-by ppr:1:core:pe=4' \
    -D ENABLE_ALBANY_PYTHON:BOOL=ON \
    -D PYTHON_EXECUTABLE='/home/projects/x86-64/python/3.7.3/bin/python' \
..
