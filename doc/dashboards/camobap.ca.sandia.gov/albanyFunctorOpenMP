set (BUILD_ALB32_NOEPETRA FALSE)
set (BUILD_ALB64 FALSE)
set (BUILD_ALB64_NOEPETRA FALSE)
set (BUILD_ALBFUNCTOR FALSE)
set (BUILD_ALBFUNCTOR_OPENMP TRUE)

