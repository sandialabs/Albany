@article{Higham:2001,
  Author =	 {Higham, NJ},
  Title =	 {{Evaluating Pade approximants of the matrix
                  logarithm}},
  Journal =	 {{SIAM Journal on Matrix Analysis and Applications}},
  Year =	 {{2001}},
  Volume =	 {{22}},
  Number =	 {{4}},
  Pages =	 {{1126-1135}},
  Month =	 {{APR 12}},
  Abstract =	 {{The inverse scaling and squaring method for
                  evaluating the logarithm of a matrix takes repeated
                  square roots to bring the matrix close to the
                  identity, computes a Pade approximant, and then
