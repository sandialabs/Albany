@article{Park.Ravani:1997,
  Author =	 {Park, FC and Ravani, B},
  Title =	 {Smooth invariant interpolation of rotations},
  Journal =	 {ACM Transactions on Graphics},
  Year =	 1997,
  Volume =	 16,
  Number =	 3,
  Pages =	 {277-295},
  Month =	 {JUL},
  Abstract =	 {We present an algorithm for generating a
                  twice-differentiable curve on the rotation group
                  SO(3) that interpolates a given ordered set of
                  rotation matrices at their specified knot times. In
                  our approach we regard SO(3) as a Lie group with a
